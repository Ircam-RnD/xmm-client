{	
	"source": {
    "include": [
    	"README.md",
    	"src/phrase",
    	"src/gmm",
    	"src/hhmm"
    ],
    "exclude": [ "src/utils" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"destination": "./doc",
		"encoding": "utf8",
		"template": "node_modules/jsdoc-template"
	},
	"templates": {
		"default": {
			"useLongnameInNav": true
		}
	}
}
